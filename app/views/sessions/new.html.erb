<div>
  <h1>ログインフォーム</h1>
</div>

<div>
  <%= form_with(url: login_path, scope: :session, local: true) do |f| %>
    
    <div>
      <%= f.label :email, "メールアドレス" %>
      <%= f.email_field :email %>
    </div>
    
    <div>
      <%= f.label :password, "パスワード" %>
      <%= f.password_field :password %>
    </div>
    
    <%= f.submit "ログイン" %>
  <% end %>
  
  <p>ユーザ登録は<%= link_to "こちら", signup_path %> </p>
</div>